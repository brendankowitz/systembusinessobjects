<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="System.BusinessObjects.Membership" namespace="System.BusinessObjects.Membership">
    <class name="Application" table="aspnet_Applications" dynamic-insert="true" dynamic-update="true">
    <id name="ID" type="System.Guid" column="ApplicationId">
      <generator class="guid"></generator>
    </id>
    <property name="ApplicationName" column="ApplicationName" type="System.String" not-null="true" length="256"/>
    <property name="LoweredApplicationName" column="LoweredApplicationName" type="System.String" not-null="true" length="256"/>
    <property name="Description" column="Description" type="System.String" not-null="false" length="256"/>
 </class>
</hibernate-mapping>
