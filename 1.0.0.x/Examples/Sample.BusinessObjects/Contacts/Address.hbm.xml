<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Sample.BusinessObjects" namespace="Sample.BusinessObjects.Contacts" >
	<class name="Address" table="addresses">

		<id name="ID" column="addressid" type="Int32">
			<generator class="native" />
		</id>

		<property name="Address1" length="255" not-null="true" />
		<property name="Suburb" length="255" not-null="true" />
		<property name="State" length="15" />
		<property name="Postcode" length="6" not-null="true" />

	</class>
</hibernate-mapping>
