<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<!-- Add this element -->
	<configSections>
		<section
			name="hibernate-configuration"
			type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate"
        />
		<section name="NHibernateSessionProvider" type="System.BusinessObjects.Providers.ProviderSectionHandler, System.BusinessObjects.Framework"/>

	</configSections>

	<NHibernateSessionProvider defaultProvider="NHibernateThreadSlotProvider">
		<providers>
			<add name="NHibernateThreadSlotProvider" type="System.BusinessObjects.Data.NHibernateThreadSlotProvider, System.BusinessObjects.Framework" connectionStringName="NpgsqlConnectionString"/>
		</providers>
	</NHibernateSessionProvider>

</configuration>